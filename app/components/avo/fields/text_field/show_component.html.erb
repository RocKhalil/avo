<%= field_wrapper **field_wrapper_args do %>
  <% if @field.as_html %>
    <%== @field.value %>
  <% elsif @field.protocol.present? %>
    <% if @field.protocol.to_sym == :link %>
      <%= link_to @field.value, @field.value %>
    <% else %>
      <%= link_to @field.value, "#{@field.protocol}:#{@field.value}" %>
    <% end %>
  <% else %>
    <%= @field.value %>
  <% end %>
<% end %>
